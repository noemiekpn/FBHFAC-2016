{
  "name": "We dont know yet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is made to be cool",
  "homepage_url": "https://www/google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "geolocation"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["src/inject/inject.css"]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}